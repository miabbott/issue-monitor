{
  "name": "security-monitor",
  "searchPhrases": [
    "security vulnerability",
    "CVE-",
    "security issue"
  ],
  "excludedRepos": [
    "example/test-repo",
    "spam-owner/spam-repo"
  ],
  "excludedOrgs": [
    "test-organization",
    "spam-org"
  ],
  "lookbackHours": 24,
  "notifications": {
    "githubIssues": {
      "enabled": true
    },
    "slack": {
      "enabled": false,
      "webhookUrl": "",
      "channel": "#security-alerts",
      "username": "GitHub Monitor",
      "iconEmoji": ":warning:"
    }
  }
}